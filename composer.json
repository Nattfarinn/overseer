    "extra": {
        "class": "Overseer\\Composer\\OverseerPlugin",
        "plugin-modifies-downloads": true,
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    },
    "bin": [
        "bin/overseer"
    ]
